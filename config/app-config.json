{
  "fs": {
    "dlDir": "./download",
    "histDir": "./download/_history",
    "toolDir": "./tool",
    "orderBat": "Order.bat",
    "orderPs1": "Order.ps1",
    "orderLst": "Order.lst"
  },
  "organize": {
    "assortRule": [
      {"fileExt": ".jpg", "fileType": "image"},
      {"fileExt": ".png", "fileType": "image"},
      {"fileExt": ".gif", "fileType": "gif"},
      {"fileExt": ".swf", "fileType": "swf"},
      {"fileExt": ".webm", "fileType": "video"},
      {"fileExt": ".ogv", "fileType": "video"},
      {"fileExt": ".mp4", "fileType": "video"}
    ],
    "fileThre": 500,
    "msg": {
      "startMsg": "File organization started.",
      "finMsg": "File organization finished."
    }
  },
  "api": {
    "uri": {
      "addItemUri": "/addItem",
      "refillUri": "/refillQueue",
      "organizeUri": "/organizeFile",
      "chkDbUri": "/checkDb",
      "getLinkUri": "/getLink"
    },
    "msg": {
      "exceptMsg": "Invalid Request",
      "startMsg": "API Server started."
    }
  },
  "req": {
    "common": {
      "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/79.0.3945.123 Safari/537.36"
    },
    "auth": {
      "userName": "user",
      "passWord": "pass",
      "authUrl": "https://api.example.com/auth/token?lang=japanese",
      "authPrefix": "Bearer "
    },
    "search": {
      "searchUrl": "https://api.example.com/posts?tags=",
      "searchParam": "+-rating:safe&limit=100&page="
    },
    "fav": {
      "favUrl": "https://api.example.com/posts/",
      "favParam": "/favorite?language=japanese"
    },
    "vote": {
      "voteUrl": "https://api.example.com/posts/",
      "voteParam": "/vote?language=japanese"
    },
    "dl": {
      "refererUrl": "https://www.example.com/ja/post/show/"
    },
    "tag": {
      "tagUrl": "https://api.example.com/posts/",
      "tagParam": "?lang=en",
      "refererUrl": "https://www.example.com/ja/post/show/"
    }
  },
  "db": {
    "tab": {
      "dlTable": "Download",
      "favTable": "Favorite"
    },
    "attr": {
      "tagAttr": "tag",
      "lastAttr": "last"
    }
  },
  "mq": {
    "url": {
      "dlQueUrl": "http://localhost:9324/queue/Download",
      "priorDlQueUrl": "http://localhost:9324/queue/priorDownload",
      "favQueUrl": "http://localhost:9324/queue/Favorite",
      "priorFavQueUrl": "http://localhost:9324/queue/priorFavorite"
    },
    "poll": {
      "dlQuePoll": "10",
      "favQuePoll": "10"
    },
    "msg": {
      "startMsg": "MQ refilling started: ",
      "finMsg": "MQ refilling finished: "
    }
  }
}
